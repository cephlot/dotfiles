Xft.dpi:				96
Xft.antialias:			1
Xft.rgba:				rgba
Xft.autohint:			0
Xft.lcdfilter:			lcddefault
Xft.hinting:			1
Xft.hintstyle:			hintfull

!*.font: 			xft:scientifica:pixelsize=12

URxvt.letterSpace: 2
URxvt.internalBorder:	30
